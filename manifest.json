{
    "name": "Add to Wishlist",
    "description": "Add to products to your Amazon Wishlist",
    "version": "1.0",
    "manifest_version": 3,
    "icons": 
        {"16": "icons/icon16.png",
          "48": "icons/icon48.png",
          "128": "icons/icon128.png"},
    "background": {
        "service_worker": "background.js"
      },
    "permissions": ["storage","tabs","activeTab","scripting"],
    "action": {
      "default_popup": "popup.html"
    }
}